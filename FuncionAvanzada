public class FuncionesAvanzadas {

    public static void main(String[] args){
        int param1= 10;
        int param2= 5;
        int param3= 1;

        suma ( param1, param2, param3);

    }

    private static void suma(int param1, int param2, int param3) {
        System.out.println(param1 + param2 + param3);
        System.out.println(param1 - param2 - param3);
        System.out.println(param1 * param2 * param3);
        System.out.println(param1 / param2 / param3);
    }

}
